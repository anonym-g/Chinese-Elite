  "relationships": [
    {
      "source": "第五届全国人民代表大会常务委员会",
      "target": "国务委员",
      "type": "PUSHED",
      "properties": {
        "start_date": "1982-05-04",
        "description": {
          "zh-cn": "1982年5月4日，第五届全国人民代表大会常务委员会第二十三次会议决定增设“国务委员”职务。",
          "en": "On May 4, 1982, the 23rd meeting of the Standing Committee of the 5th National People's Congress decided to establish the post of 'State Councilor'."
        }
      }
    },
    {
      "source": "国务委员",
      "target": "中华人民共和国国务院",
      "type": "SUBORDINATE_OF",
      "properties": {
        "description": {
          "zh-cn": "国务委员是中华人民共和国国务院的组成人员。",
          "en": "The State Councilor is a member of the State Council of the People's Republic of China."
        }
      }
    },
    {
      "source": "国务委员",
      "target": "中华人民共和国国务院总理",
      "type": "SUBORDINATE_OF",
      "properties": {
        "description": {
          "zh-cn": "国务委员协助国务院总理工作，受总理或国务院常务会议的委托，负责某些方面的工作或重大的专项任务。",
          "en": "The State Councilor assists the Premier of the State Council and is responsible for certain areas of work or major special tasks as entrusted by the Premier or the Standing Committee of the State Council."
        }
      }
    },
    {
      "source": "国务委员",
      "target": "国务院常务会议",
      "type": "MEMBER_OF",
      "properties": {
        "description": {
          "zh-cn": "国务委员是国务院常务会议的组成人员。",
          "en": "The State Councilor is a member of the Standing Committee of the State Council."
        }
      }
    },
    {
      "source": "国务委员",
      "target": "国务院全体会议",
      "type": "MEMBER_OF",
      "properties": {
        "description": {
          "zh-cn": "国务委员作为国务院组成人员，是国务院全体会议的成员。",
          "en": "As a member of the State Council, the State Councilor is a member of the Plenary Meeting of the State Council."
        }
      }
    },
    {
      "source": "中华人民共和国国务院总理",
      "target": "国务委员",
      "type": "PUSHED",
      "properties": {
